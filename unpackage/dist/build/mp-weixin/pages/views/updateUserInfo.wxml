<view class="info container"><view class="form"><view class="head boxshow3"><image mode="aspectFill" src="{{info.avatar_url?info.avatar_url:'/static/images/icon/head.png'}}"></image><image class="icon" src="/static/images/icon/paiz.png" data-event-opts="{{[['tap',[['upimg',['$event']]]]]}}" bindtap="__e"></image></view><block wx:if="{{userType==2}}"><view class="item"><view class="hint">工号</view><view class="value"><input type="text" placeholder="请输入工号" data-key="work_no" placeholder-class="plac" data-event-opts="{{[['input',[['setInpt',['$event']]]]]}}" value="{{info.work_no}}" bindinput="__e"/></view></view></block><view class="item"><view class="hint">姓名</view><view class="value"><input type="text" placeholder="请输入姓名" data-key="name" placeholder-class="plac" data-event-opts="{{[['input',[['setInpt',['$event']]]]]}}" value="{{info.name}}" bindinput="__e"/></view></view><block wx:if="{{userType==2}}"><view class="item"><view class="hint">初始提成</view><view class="value"><input type="number" placeholder="请输入初始提成(1-100)" data-key="tichen" placeholder-class="plac" data-event-opts="{{[['input',[['setInpt',['$event']]]]]}}" value="{{info.tichen}}" bindinput="__e"/></view></view></block><view class="item"><view class="hint">手机号</view><view class="value"><input type="number" placeholder="请输入手机号" data-key="phone" placeholder-class="plac" data-event-opts="{{[['input',[['setInpt',['$event']]]]]}}" value="{{info.phone}}" bindinput="__e"/></view></view><view class="item"><view class="hint">性别</view><view class="value"><picker mode="selector" range="{{sexList}}" range-key="label" value="{{sexIndex}}" data-event-opts="{{[['change',[['changeP',['$event']]]]]}}" bindchange="__e"><block wx:if="{{sexIndex==0}}"><view class="hint">{{sexList[sexIndex].label}}</view></block><block wx:else><view>{{sexList[sexIndex].label}}</view></block></picker><van-icon class="arrow" vue-id="fee6956c-1" name="arrow" bind:__l="__l"></van-icon></view></view><view class="item"><view class="hint">出生日期</view><view class="value"><picker mode="date" value="{{info.birthday}}" data-event-opts="{{[['change',[['changeDate',['$event']]]]]}}" bindchange="__e"><block wx:if="{{info.birthday}}"><view>{{info.birthday}}</view></block><block wx:else><view class="hint">请选择出生日期</view></block></picker></view></view><view class="item"><view class="hint">职位</view><view class="value"><input type="text" placeholder="请输入职位" data-key="job" placeholder-class="plac" data-event-opts="{{[['input',[['setInpt',['$event']]]]]}}" value="{{info.job}}" bindinput="__e"/></view></view><view class="item"><view class="hint">备注</view><view class="value"><input type="text" placeholder="请输入备注" data-key="note" placeholder-class="plac" data-event-opts="{{[['input',[['setInpt',['$event']]]]]}}" value="{{info.note}}" bindinput="__e"/></view></view><view class="item"><view class="hint">状态</view><view class="value"><text class="{{[(!switchChecked)?'hint':'']}}">{{switchText}}</text><switch style="transform:scale(0.7);" checked="{{switchChecked}}" data-event-opts="{{[['change',[['changeSwitch',['$event']]]]]}}" bindchange="__e"></switch></view></view><view class="confirm"><van-button vue-id="fee6956c-2" round="{{true}}" color="#33b793" type="primary" block="{{true}}" data-event-opts="{{[['tap',[['setUpdateInfo',['$event']]]]]}}" bindtap="__e" bind:__l="__l" vue-slots="{{['default']}}">确 定</van-button></view></view></view>